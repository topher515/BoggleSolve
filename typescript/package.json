{
  "name": "bogglesolve",
  "version": "1.0.0",
  "scripts": {
    "build": "rimraf dist && tsc",
    "watch": "tsc --watch --preserveWatchOutput --pretty",
    "dev": "cross-env NODE_ENV=development nodemon",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "node -r tsconfig-paths/register dist/boggle_solve.js",
