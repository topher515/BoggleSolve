  },
  "license": "MIT",
  "dependencies": {
    "@types/seedrandom": "^3.0.1",
    "lodash": "^4.17.21",
    "prefix-trie-ts": "^0.0.4",
    "seedrandom": "^3.0.5",
    "trie": "^0.2.1",
    "ts-command-line-args": "^2.1.0",
    "ts-node": "^10.1.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.14.5",
    "@types/lodash": "^4.14.172",
    "@types/node": "latest",
    "rimraf": "^3.0.2",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
